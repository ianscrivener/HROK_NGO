<h1>Listing locations</h1>

<table>
  <tr>
    <th>Address name</th>
    <th>Address1</th>
    <th>Address2</th>
    <th>State</th>
    <th>Suburb</th>
    <th>Country</th>
    <th>Country code</th>
    <th>Postcode</th>
    <th>Is main</th>
    <th>Geo admin 0</th>
    <th>Geo admin 1</th>
    <th>Geo admin 2</th>
    <th>Geo admin 3</th>
    <th>Geo admin 4</th>
    <th>Geo dlat</th>
    <th>Geo dlong</th>
    <th>Geo geocode modified</th>
    <th>Geo geocode reqd</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @locations.each do |location| %>
  <tr>
    <td><%= location.address_name %></td>
    <td><%= location.address1 %></td>
    <td><%= location.address2 %></td>
    <td><%= location.state %></td>
    <td><%= location.suburb %></td>
    <td><%= location.country %></td>
    <td><%= location.country_code %></td>
    <td><%= location.postcode %></td>
    <td><%= location.is_main %></td>
    <td><%= location.geo_admin_0 %></td>
    <td><%= location.geo_admin_1 %></td>
    <td><%= location.geo_admin_2 %></td>
    <td><%= location.geo_admin_3 %></td>
    <td><%= location.geo_admin_4 %></td>
    <td><%= location.geo_dlat %></td>
    <td><%= location.geo_dlong %></td>
    <td><%= location.geo_geocode_modified %></td>
    <td><%= location.geo_geocode_reqd %></td>
    <td><%= link_to 'Show', location %></td>
    <td><%= link_to 'Edit', edit_location_path(location) %></td>
    <td><%= link_to 'Destroy', location, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

